{
  "name": "appleple/acms-google-sheets",
  "description": "Google Calendar API と連携し、お問い合わせフォームなどで送信された内容を任意のGoogleカレンダーに登録できます。",
  "keywords": [
    "SMS",
    "a-blog cms"
  ],
  "homepage": "https://www.a-blogcms.jp",
  "scripts": {
    "post-update-cmd": "Google_Task_Composer::cleanup"
  },
  "extra": {
    "google/apiclient-services": [
      "Calendar"
    ]
  },
  "require": {
    "php": ">=7.2.5",
    "google/apiclient": "^2.14"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "config": {
    "platform": {
      "php": "7.2.5"
    }
  }
}
